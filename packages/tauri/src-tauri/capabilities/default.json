{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "デスクトップアプリケーションのメイン機能",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "store:default",
    "deep-link:default",
    {
      "identifier": "opener:allow-open-url",
      "allow": [{ "url": "https://**" }, { "url": "http://**" }]
    }
  ]
}
